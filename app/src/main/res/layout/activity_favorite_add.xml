<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#FFFFFF">


    <View
        android:id="@+id/view_bgr"
        android:layout_width="383dp"
        android:layout_height="585dp"
        android:layout_marginStart="5dp"
        android:layout_marginTop="60dp"
        android:layout_marginEnd="5dp"
        android:layout_marginBottom="5dp"
        android:background="@drawable/rounded_rec_gray"
        app:layout_constraintBottom_toTopOf="@+id/bottom_navigation_view"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="1.0" />

    <View
        android:id="@+id/view_settime"
        android:layout_width="365dp"
        android:layout_height="302dp"
        android:layout_marginStart="14dp"
        android:layout_marginTop="124dp"
        android:layout_marginEnd="14dp"
        android:background="@drawable/rounded_rec_white"
        android:elevation="2dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <View
        android:id="@+id/view_favorite"
        android:layout_width="365dp"
        android:layout_height="103dp"
        android:layout_marginStart="14dp"
        android:layout_marginTop="59dp"
        android:layout_marginEnd="14dp"
        android:background="@drawable/top_rounded_rec"
        android:elevation="3dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.119" />

    <View
        android:id="@+id/view_setStarttime"
        android:layout_width="113dp"
        android:layout_height="50dp"
        android:layout_marginStart="20dp"
        android:layout_marginTop="118dp"
        android:layout_marginEnd="232dp"
        android:layout_marginBottom="134dp"
        android:background="@drawable/left_rounded_rec"
        android:elevation="4dp"
        app:layout_constraintBottom_toBottomOf="@+id/view_settime"
        app:layout_constraintEnd_toEndOf="@+id/view_settime"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/view_settime"
        app:layout_constraintTop_toTopOf="@+id/view_settime"
        app:layout_constraintVertical_bias="0.0" />

    <View
        android:id="@+id/view_setEndtime"
        android:layout_width="113dp"
        android:layout_height="50dp"
        android:layout_marginStart="20dp"
        android:layout_marginTop="178dp"
        android:layout_marginEnd="232dp"
        android:layout_marginBottom="74dp"
        android:background="@drawable/left_rounded_rec"
        android:elevation="4dp"
        app:layout_constraintBottom_toBottomOf="@+id/view_settime"
        app:layout_constraintEnd_toEndOf="@+id/view_settime"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/view_settime"
        app:layout_constraintTop_toTopOf="@+id/view_settime"
        app:layout_constraintVertical_bias="0.0" />

    <View
        android:id="@+id/view_startTimeInput"
        android:layout_width="205dp"
        android:layout_height="50dp"
        android:layout_marginStart="7dp"
        android:layout_marginTop="118dp"
        android:layout_marginEnd="20dp"
        android:layout_marginBottom="134dp"
        android:background="@drawable/right_rounded_rec"
        android:elevation="4dp"
        app:layout_constraintBottom_toBottomOf="@+id/view_settime"
        app:layout_constraintEnd_toEndOf="@+id/view_settime"
        app:layout_constraintStart_toEndOf="@+id/view_setStarttime"
        app:layout_constraintTop_toTopOf="@+id/view_settime" />

    <View
        android:id="@+id/view_endTimeInput"
        android:layout_width="205dp"
        android:layout_height="50dp"
        android:layout_marginStart="7dp"
        android:layout_marginTop="178dp"
        android:layout_marginEnd="20dp"
        android:layout_marginBottom="74dp"
        android:background="@drawable/right_rounded_rec"
        android:elevation="4dp"
        app:layout_constraintBottom_toBottomOf="@+id/view_settime"
        app:layout_constraintEnd_toEndOf="@+id/view_settime"
        app:layout_constraintStart_toEndOf="@+id/view_setEndtime"
        app:layout_constraintTop_toTopOf="@+id/view_settime" />

    <com.google.android.material.textfield.TextInputEditText
        android:id="@+id/editTextStartTime"
        android:layout_width="189dp"
        android:layout_height="33dp"
        android:layout_marginStart="5dp"
        android:layout_marginTop="5dp"
        android:layout_marginEnd="5dp"
        android:layout_marginBottom="5dp"
        android:elevation="5dp"
        android:hint="12:00"
        android:inputType="time"
        android:textSize="12sp"
        app:layout_constraintBottom_toBottomOf="@+id/view_startTimeInput"
        app:layout_constraintEnd_toEndOf="@+id/view_startTimeInput"
        app:layout_constraintStart_toStartOf="@+id/view_startTimeInput"
        app:layout_constraintTop_toTopOf="@+id/view_startTimeInput" />

    <View
        android:id="@+id/view_add"
        android:layout_width="325dp"
        android:layout_height="50dp"
        android:layout_marginStart="20dp"
        android:layout_marginTop="238dp"
        android:layout_marginEnd="20dp"
        android:layout_marginBottom="14dp"
        android:background="@drawable/rounded_rec_gray"
        android:elevation="10dp"
        app:layout_constraintBottom_toBottomOf="@+id/view_settime"
        app:layout_constraintEnd_toEndOf="@+id/view_settime"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="@+id/view_settime"
        app:layout_constraintTop_toTopOf="@+id/view_settime"
        app:layout_constraintVertical_bias="1.0" />

    <TextView
        android:id="@+id/favorite"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="40dp"
        android:layout_marginTop="10dp"
        android:layout_marginEnd="40dp"
        android:layout_marginBottom="600dp"
        android:fontFamily="sans-serif"
        android:text="즐겨찾기"
        android:textSize="20sp"
        app:layout_constraintBottom_toBottomOf="@+id/view_bgr"
        app:layout_constraintEnd_toEndOf="@+id/view_bgr"
        app:layout_constraintHorizontal_bias="0.481"
        app:layout_constraintStart_toStartOf="@+id/view_bgr"
        app:layout_constraintTop_toTopOf="@+id/view_bgr"
        app:layout_constraintVertical_bias="0.0" />

    <TextView
        android:id="@+id/selectedStation"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="32dp"
        android:layout_marginEnd="33dp"
        android:layout_marginBottom="58dp"
        android:elevation="5dp"
        android:text="SelectedStation"
        android:textSize="20sp"
        app:layout_constraintBottom_toBottomOf="@+id/view_favorite"
        app:layout_constraintEnd_toEndOf="@+id/view_favorite"
        app:layout_constraintStart_toStartOf="@+id/view_favorite"
        app:layout_constraintTop_toTopOf="@id/view_favorite" />

    <TextView
        android:id="@+id/selectedBus"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="32dp"
        android:layout_marginTop="55dp"
        android:layout_marginEnd="33dp"
        android:layout_marginBottom="8dp"
        android:elevation="5dp"
        android:text="SelectedBus"
        android:textSize="20sp"
        app:layout_constraintBottom_toBottomOf="@+id/view_favorite"
        app:layout_constraintEnd_toEndOf="@+id/view_favorite"
        app:layout_constraintStart_toStartOf="@+id/view_favorite"
        app:layout_constraintTop_toTopOf="@id/view_favorite" />

    <TextView
        android:id="@+id/start"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="26dp"
        android:layout_marginTop="5dp"
        android:layout_marginEnd="27dp"
        android:layout_marginBottom="5dp"
        android:elevation="5dp"
        android:text="Start"
        android:textSize="15sp"
        app:layout_constraintBottom_toBottomOf="@+id/view_setStarttime"
        app:layout_constraintEnd_toEndOf="@+id/view_setStarttime"
        app:layout_constraintStart_toStartOf="@+id/view_setStarttime"
        app:layout_constraintTop_toTopOf="@+id/view_setStarttime" />

    <TextView
        android:id="@+id/end"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="26dp"
        android:layout_marginTop="5dp"
        android:layout_marginEnd="27dp"
        android:layout_marginBottom="5dp"
        android:elevation="5dp"
        android:text="End"
        android:textSize="15sp"
        app:layout_constraintBottom_toBottomOf="@+id/view_setEndtime"
        app:layout_constraintEnd_toEndOf="@+id/view_setEndtime"
        app:layout_constraintHorizontal_bias="0.4"
        app:layout_constraintStart_toStartOf="@+id/view_setEndtime"
        app:layout_constraintTop_toTopOf="@+id/view_setEndtime"
        app:layout_constraintVertical_bias="0.35" />

    <TextView
        android:id="@+id/add"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="132dp"
        android:layout_marginTop="5dp"
        android:layout_marginEnd="133dp"
        android:layout_marginBottom="5dp"
        android:elevation="11dp"
        android:text="Add"
        android:textSize="15sp"
        app:layout_constraintBottom_toBottomOf="@+id/view_add"
        app:layout_constraintEnd_toEndOf="@+id/view_add"
        app:layout_constraintHorizontal_bias="0.855"
        app:layout_constraintStart_toStartOf="@+id/view_add"
        app:layout_constraintTop_toTopOf="@+id/view_add" />

    <ImageView
        android:id="@+id/imageView_close"
        android:layout_width="20dp"
        android:layout_height="20dp"
        android:layout_marginStart="335dp"
        android:layout_marginTop="15dp"
        android:layout_marginEnd="10dp"
        android:layout_marginBottom="68dp"
        android:elevation="5dp"
        app:layout_constraintBottom_toBottomOf="@+id/view_favorite"
        app:layout_constraintEnd_toEndOf="@+id/view_favorite"
        app:layout_constraintStart_toStartOf="@+id/view_favorite"
        app:layout_constraintTop_toTopOf="@id/view_favorite"
        app:srcCompat="@drawable/ic_x" />

    <ImageView
        android:id="@+id/imageView_bookmark"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="9dp"
        android:layout_marginTop="15dp"
        android:layout_marginEnd="340dp"
        android:layout_marginBottom="68dp"
        android:elevation="5dp"
        app:layout_constraintBottom_toBottomOf="@+id/view_favorite"
        app:layout_constraintEnd_toEndOf="@+id/view_favorite"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/view_favorite"
        app:layout_constraintTop_toTopOf="@id/view_favorite"
        app:layout_constraintVertical_bias="0.25"
        app:srcCompat="@drawable/ic_bookmark" />

    <ImageView
        android:id="@+id/imageView_back"
        android:layout_width="25dp"
        android:layout_height="25dp"
        android:layout_marginStart="5dp"
        android:layout_marginTop="10dp"
        android:layout_marginEnd="340dp"
        android:layout_marginBottom="590dp"
        app:layout_constraintBottom_toBottomOf="@+id/view_bgr"
        app:layout_constraintEnd_toEndOf="@+id/view_bgr"
        app:layout_constraintHorizontal_bias="0.937"
        app:layout_constraintStart_toStartOf="@+id/view_bgr"
        app:layout_constraintTop_toTopOf="@+id/view_bgr"
        app:layout_constraintVertical_bias="0.0"
        app:srcCompat="@drawable/ic_back" />

    <com.google.android.material.bottomnavigation.BottomNavigationView
        android:id="@+id/bottom_navigation_view"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        app:layout_constraintBottom_toBottomOf="parent"

        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHeight_percent="0.1"
        app:menu="@menu/bottom_navigation_menu" />

    <com.google.android.material.textfield.TextInputEditText
        android:id="@+id/editTextEndTime"
        android:layout_width="189dp"
        android:layout_height="33dp"
        android:layout_marginStart="5dp"
        android:layout_marginTop="10dp"
        android:layout_marginEnd="5dp"
        android:layout_marginBottom="2dp"
        android:elevation="5dp"
        android:hint="12:00"
        android:inputType="time"
        android:textSize="12sp"
        app:layout_constraintBottom_toBottomOf="@+id/view_endTimeInput"
        app:layout_constraintEnd_toEndOf="@+id/view_endTimeInput"
        app:layout_constraintHorizontal_bias="0.727"
        app:layout_constraintStart_toStartOf="@+id/view_endTimeInput"
        app:layout_constraintTop_toTopOf="@+id/view_endTimeInput"
        app:layout_constraintVertical_bias="0.0" />


